		"@types/reveal.js": "^4.4.8",
		"reveal.js": "^5.0.2",
		"sharp": "^0.33.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vite-image": "^1.0.2",
		"vite-imagetools": "^6.2.7"
	},
	"type": "module",
	"packageManager": "yarn@4.0.2"
